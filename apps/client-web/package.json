{
  "name": "@officeclaw/client-web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "assets:sync": "node ../../tools/sync-runtime-assets.mjs --source-glb ../../assets/glb --source-scenes ../../assets/scenes --target-assets public/assets --target-scenes public/scenes",
    "assets:sync:prune": "node ../../tools/sync-runtime-assets.mjs --source-glb ../../assets/glb --source-scenes ../../assets/scenes --target-assets public/assets --target-scenes public/scenes --prune",
    "assets:verify": "node ../../tools/sync-runtime-assets.mjs --source-glb ../../assets/glb --source-scenes ../../assets/scenes --target-assets public/assets --target-scenes public/scenes --check"
  },
  "dependencies": {
    "@react-three/fiber": "^8.17.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "three": "^0.166.1",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "@types/three": "^0.166.0",
    "@vitejs/plugin-react": "^4.3.1",
    "typescript": "^5.5.4",
    "vite": "^5.4.1"
  }
}
